<?jelly escape-by-default='true'?>
<j:if test="${it.isTagged()}" xmlns:j="jelly:core">
  <a href="${rootURL}/${it.build.url}tagBuild/">
    <img width="16" height="16"
      tooltip="${it.tooltip}"
      alt="[tagged]"
      src="${imagesURL}/16x16/save.png"/>
  </a>
</j:if>
